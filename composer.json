{
    "name": "lewiscowles/file-extension-utils",
    "description": "Utilities for dealing with file extensions",
    "type": "library",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "phpunit/phpunit": "^6.0",
        "php-coveralls/php-coveralls": "*"
    },
    "autoload": {
        "psr-4": { "lewiscowles\\Utils\\FileSystem\\Extension\\": "src" },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": { "lewiscowles\\Utils\\FileSystem\\Extension\\Tests\\": "tests" }
    },
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Lewis Cowles",
            "email": "lewiscowles@me.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin/"
    }
}
